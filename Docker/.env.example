# Copy this file to .env and update the values

# Database Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=YourStrongPostgresPassword
POSTGRES_DB=postgres

MONGO_ROOT_USERNAME=mongo
MONGO_ROOT_PASSWORD=YourStrongMongoPassword

# Redis Configuration
REDIS_PASSWORD=YourStrongRedisPassword

# RabbitMQ Configuration
RABBITMQ_USER=battleuser
RABBITMQ_PASSWORD=YourStrongRabbitPassword

# Aspire Dashboard Configuration
ASPIRE_DASHBOARD_OTLP_API_KEY=your-generated-api-key-here

# Application Configuration
ASPNETCORE_ENVIRONMENT=Development
OTEL_SERVICE_VERSION=1.0.0

# Network Ports
