ASPIRE_DASHBOARD_UI_PORT=18888
ASPIRE_DASHBOARD_OTLP_PORT=18889
GATEWAY_PORT=8080
USERSERVICE_PORT=5001
GAMESERVICE_PORT=5002
MATCHSERVICE_PORT=5003
LEADERBOARDSERVICE_PORT=5004

# GitHub User
GITHUB_USER=your-github-username